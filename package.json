  "license": "Apache License",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.16.0",
    "tslint": "^3.8.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10"
  }
}
