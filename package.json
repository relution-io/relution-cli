{
  "name": "relution-cli",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run tslint",
    "tslint": "tslint src/**/*.ts",
    "test": "istanbul cover lib/**/*.js && node testrunner.js",
    "testfile": "jasmine --verbose --autotest --watch --color --test-dir lib/gii/templates/new",
    "clean": "del 'lib/**'",
    "build": "tsc -p .",
    "api": "typedoc -p . --rootDir src --out public/docs --module commonjs --stripInternal --name Relution-CLI --exclude **/*.spec.ts src typings/main.d.ts",
    "watch": "npm run clean && tsconfig && tsc -p . -w",
    "postinstall": "typings install"
  },
  "bin": {
    "relution": "lib/cli.js"
  },
  "author": "Pascal Brewing",
  "license": "Apache License",
  "devDependencies": {
    "del-cli": "^0.2.0",
    "istanbul": "^0.4.3",
    "jasmine": "^2.2.1",
    "tslint": "^3.8.1",
    "typedoc": "sierrasoftworks/typedoc#v1.8.10",
    "typescript": "^1.8.10"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.0.0-beta.7",
    "chalk": "^1.1.3",
    "cli-loader": "^1.1.1",
    "cli-table": "^0.3.1",
    "cli-table2": "^0.2.0",
    "common-tags": "^0.1.1",
    "figures": "^1.5.0",
    "hjson": "^1.8.4",
    "inquirer": "DrMabuse23/Inquirer.js#add78131e3461acbc861b4cdba8aff67a54ef4f4",
    "install": "^0.6.1",
    "lodash": "^4.11.2",
    "node-uuid": "^1.4.7",
    "nopt": "^3.0.6",
    "npm": "^3.9.0",
    "relution-sdk": "git+ssh://git@github.com/relution-io/relution-sdk.git",
    "rimraf": "^2.5.2",
    "username": "^2.1.0"
  }
}
