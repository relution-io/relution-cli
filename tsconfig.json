{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "declaration": false,
        "sourceMap": true,
        "rootDir": "./src",
        "outDir": "./lib",
        "pretty": true,
        "stripInternal": true,
        "noEmitOnError": false,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true
    },
    "filesGlob": [
        "src/**/*.ts",
        "!src/**/*.d.ts",
        "typings/main.d.ts"
    ],
    "files": [
        "typings/main.d.ts",
        "src/cli.ts",
        "src/commands/Environment.ts",
        "src/commands/Server.ts",
        "src/commands/server/ServerCrud.ts",
        "src/commands/Tower.spec.ts",
        "src/commands/Tower.ts",
        "src/gii/Gii.spec.ts",
        "src/gii/Gii.ts",
        "src/gii/templates/EnvironmentTemplate.ts",
        "src/models/CertModelRc.ts",
        "src/models/ServerModelRc.spec.ts",
        "src/models/ServerModelRc.ts",
        "src/utility/Command.spec.ts",
        "src/utility/Command.ts",
        "src/utility/FileApi.spec.ts",
        "src/utility/FileApi.ts",
        "src/utility/InquirerHelper.ts",
        "src/utility/Table.ts",
        "src/utility/Translation.ts",
        "src/utility/UserRc.ts",
        "src/utility/Validator.ts",
        "src/utility/Welcome.ts"
    ]
}
